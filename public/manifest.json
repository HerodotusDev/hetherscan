{
  "manifest_version": 3,
  "name": "hetherscan",
  "version": "1.0.0",
  "description": "Grants etherscan more functionality, including full diamond standard support & Herodotus proving services",
  "content_scripts": [
    {
      "matches": ["https://sepolia.etherscan.io/address/*"],
      "js": ["content-scripts/diamond.content.js"]
    },
    {
      "matches": ["https://sepolia.etherscan.io/*"],
      "js": ["content-scripts/inject.content.js"]
    },
    {
      "matches": ["https://sepolia.etherscan.io/*"],
      "js": ["content-scripts/test.content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["https://sepolia.etherscan.io/*"]
    }
  ],
  "permissions": []
}
